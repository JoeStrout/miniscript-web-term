{
  "name": "miniscript-web-term",
  "version": "0.2.1",
  "description": "A web-based MiniScript terminal runner",
  "author": "Sebastian Nozzi",
  "license": "MIT",
  "keywords": [
    "miniscript",
    "xtermjs",
    "runner",
    "commandline",
    "terminal"
  ],
  "files": [
    "dist",
    "lib",
    "example",
    "index.html"
  ],
  "scripts": {
    "build": "esbuild src/index.ts --bundle --sourcemap --outfile=dist/miniscript-web-term.js",
    "build-watch": "esbuild src/index.ts --bundle --sourcemap --outfile=dist/miniscript-web-term.js --watch"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.4",
    "esbuild": "0.19.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@xterm/xterm": "^5.4.0-beta.1",
    "miniscript-ts": "^0.1.9",
    "xterm-readline": "^1.1.1"
  }
}
